{
	"name": "@databasin/cli",
	"version": "0.4.0",
	"description": "Command-line interface for Databasin data integration platform - manage projects, connectors, pipelines, and execute SQL queries",
	"type": "module",
	"main": "./dist/index.js",
	"bin": {
		"databasin": "./dist/index.js"
	},
	"scripts": {
		"dev": "bun run src/index.ts",
		"build": "bun run build.ts",
		"build:quick": "bun build src/index.ts --outdir dist --target node",
		"build:exe": "bun build src/index.ts --compile --outfile dist/databasin --minify",
		"start": "bun run dist/index.js",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"test:unit": "bun test tests/unit tests/client tests/utils",
		"test:integration": "bun test tests/integration/",
		"test:smoke": "dist/databasin --version && dist/databasin --help && dist/databasin auth --help && dist/databasin projects --help && dist/databasin connectors --help && dist/databasin pipelines --help && dist/databasin sql --help && dist/databasin automations --help && dist/databasin update --help",
		"test:all": "bun run typecheck && bun run test:unit && bun run build:exe && bun run test:smoke",
		"verify": "bun run typecheck && bun run test && bun run build:exe && bun run test:smoke",
		"lint": "echo 'Linting not configured yet'",
		"format": "echo 'Formatting not configured yet'",
		"clean": "rm -rf dist",
		"dev:install": "cp dist/linux-x64/databasin ~/.local/bin/databasin"
	},
	"dependencies": {
		"commander": "^12.1.0",
		"prompts": "^2.4.2",
		"cli-table3": "^0.6.5",
		"chalk": "^5.3.0",
		"ora": "^8.1.1"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/prompts": "^2.4.9",
		"typescript": "^5.7.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/index.js",
		"dist/linux/",
		"dist/macos/",
		"dist/build-info.json",
		"README.md",
		"CHANGELOG.md",
		"EXAMPLES.md",
		"LICENSE"
	],
	"keywords": [
		"databasin",
		"cli",
		"data integration",
		"etl",
		"sql",
		"pipeline",
		"connector",
		"data warehouse",
		"automation",
		"bun"
	],
	"author": "Databasin",
	"license": "CC-BY-4.0",
	"homepage": "https://databasin.ai/",
	"repository": {
		"type": "git",
		"url": "https://github.com/Databasin-AI/databasin-cli"
	},
	"bugs": {
		"url": "https://github.com/Databasin-AI/databasin-cli/issues",
		"email": "support@databasin.ai"
	},
	"engines": {
		"node": ">=22.0.0",
		"bun": ">=1.0.0"
	}
}
